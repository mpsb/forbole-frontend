  "dependencies": {
    "gh-pages": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "cypress": "^13.6.1",
    "jest": "^29.7.0",
    "parcel": "^2.10.3",
    "process": "^0.11.10"
  }
